<h1>New wpis</h1>

<% form_for(@wpis) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :photospace_id %><br />
    <%= f.text_field :photospace_id %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.label :podpis %><br />
    <%= f.text_field :podpis %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', wpisy_path %>